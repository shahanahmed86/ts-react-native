{
  "name": "ts-react-native",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^18"
  },
  "engineStrict": true,
  "scripts": {
    "prepare": "echo 'installing husky...' && husky install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --ext .{ts,tsx}",
    "prettify": "prettier --config .prettierrc 'src/**/*.*'",
    "prettify:fix": "prettier --config .prettierrc 'src/**/*.*' --write",
    "prettify:watch": "onchange 'src/**/*.*' -- prettier --config .prettierrc --write {{changed}}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shahanahmed86/ts-react-native.git"
  },
  "keywords": [
    "react-native",
    "rn",
    "typescript",
    "ts"
  ],
  "author": "Shahan Ahmed Khan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shahanahmed86/ts-react-native/issues"
  },
  "homepage": "https://github.com/shahanahmed86/ts-react-native#readme",
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-jest": "^29.2.1",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "lint-staged": "^13.2.2",
    "metro-react-native-babel-preset": "0.73.9",
    "onchange": "^7.1.0",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
